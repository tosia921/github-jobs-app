{
    "extends": [
      "wesbos"
    ],
    "rules": {
      "no-console": 0,
      "import/no-named-as-default": 0,
      "prefer-destructuring": ["error", {"object": true, "array": false}],
      "react/prop-types": "off",
      "react/jsx-props-no-spreading": "off",
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es5",
          "singleQuote": true,
          "printWidth": 120,
          "tabWidth": 4,
          "endOfLine":"auto"
        }
      ]
    }
  }